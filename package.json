        "clsx": "^2.1.1",
        "dotenv": "^17.2.3",
        "lucide-vue-next": "^0.539.0",
        "mongodb": "^6.20.0",
        "pinia": "^3.0.3",
        "reka-ui": "^2.5.1",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^4.1.11",
